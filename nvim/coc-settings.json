{
	// Plugins

	// Swift
	"sourcekit.trace.server": "verbose",
	"sourcekit.commandPath": "~/Programs/sourcekit-lsp/.build/release/sourcekit-lsp",

	"sourcekit.enable": true,
	"sourcekit.args": [
		"--log-level",
		"info",
		"-Xswiftc",
		"-Xfrontend",
		"-Xswiftc",
		"-validate-tbd-against-ir=none"
	],

	// Clangd
	"clangd.semanticHighlighting": true,
	"clangd.enabled": false,

	// Python
	"python.linting.pylintEnabled": false,
	"python.linting.mypyEnabled": true,
	"python.linting.enabled": true,

	"coc.preferences.jumpCommand": "edit",
	"coc.preferences.semanticTokensHighlights": true,
	"coc.preferences.colorSupport": true,

	"diagnostic.refreshOnInsertMode": true,
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextCurrentLineOnly": true,

	"codeLens.enable": true,
	"codeLens.separator": "‣",
	"codeLens.subseparator": " | ",
	"list.indicator": "➤",

	"suggest.enablePreview": true,
	"suggest.removeDuplicateItems": true,
	"suggest.detailField": "preview",
	"suggest.detailMaxLength": 100,

	"suggest.floatConfig": {
		"border": true
	},

	"diagnostic.floatConfig": {
		"border": true
	},

	"hover.floatConfig": {
		"border": true
	},

	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\uf169",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\uf794",
		"reference": "\uf838",
		"constant": "\uf8ff",
		"method": "\uf794",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\ue60b",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\ue79b",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf02b",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\uf672",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},

	"python.autoComplete.extraPaths": [
		"${env:SPARK_HOME}/python",
		"${env:SPARK_HOME}/python/pyspark"
	],

	"cSpell.userWords": [
		"CTOML",
		"Denite",
		"Neovim",

{
	"languageserver": {
		"sourcekit-lsp": {
			"command": "/Users/lebje/Programs/sourcekit-lsp/.build/release/sourcekit-lsp",
			"args": [],
			"filetypes": ["c", "cpp", "swift", "objc", "objcpp"],
			"trace.server": "verbose",
			"initializationOptions": {},
			"settings": {}
		}
	},

	"coc.preferences.jumpCommand": "tabe",
	"coc.preferences.semanticTokensHighlights": true,
	"coc.preferences.colorSupport": true,

	"diagnostic.refreshOnInsertMode": true,
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextCurrentLineOnly": true,

	"codeLens.enable": true,
	"codeLens.separator": "‣",
	"codeLens.subseparator": " | ",
	"list.indicator": "➤",

	"suggest.enablePreview": true,
	"suggest.removeDuplicateItems": true,
	"suggest.detailField": "preview",
	"suggest.detailMaxLength": 100,
	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\uf169",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\uf794",
		"reference": "\uf838",
		"constant": "\uf8ff",
		"method": "\uf794",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\ue60b",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\ue79b",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf02b",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\uf672",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},

	// Plugins
	"clangd.semanticHighlighting": true,
	"clangd.enabled": true,

	"python.linting.pylintEnabled": false,
	"python.linting.mypyEnabled": true,
	"python.linting.enabled": true,

	"explorer.icon.enableNerdfont": true,
	"explorer.datetime.format": "eee, MMM yyyy, h:mm:ss a",
	"explorer.file.column.indent.indentLine": true,

	"explorer.keyMappings": {
		"i": false, // cancel default mapkey

		"gk": "expandablePrev",
		"gj": "expandableNext",

		"*": "toggleSelection",
		"<tab>": "actionMenu",

		"h": "collapse",
		"l": ["expandable?", "expand", "open"],
		"J": ["toggleSelection", "nodeNext"],
		"K": ["toggleSelection", "nodePrev"],
		"gl": "expandRecursive",
		"gh": "collapseRecursive",
		"<2-LeftMouse>": [
			"expandable?",
			["expanded?", "collapse", "expand"],
			"open"
		],
		"o": ["expanded?", "collapse", "expand"],
		"<cr>": ["expandable?", "cd", "open"],
		"e": "open",
		"s": "open:split",
		"S": "open:split:plain",
		"E": "open:vsplit",
		"t": "open:tab",
		"<bs>": "gotoParent",
		"gp": "preview:labeling",

		"y": "copyFilepath",
		"Y": "copyFilename",
		"c": "copyFile",
		"x": "cutFile",
		"p": "pasteFile",
		"d": "delete",
		"D": "deleteForever",

		"a": "addFile",
		"A": "addDirectory",
		"r": "rename",

		".": "toggleHidden",
		"R": "refresh",

		"?": "help",
		"q": "quit",
		"<esc>": "esc",
		"X": "systemExecute",
		"gd": "listDrive",

		"f": "search",
		"F": "searchRecursive",

		"gf": "gotoSource:file",
		"gb": "gotoSource:buffer",

		"[[": "sourcePrev",
		"]]": "sourceNext",

		"[d": "diagnosticPrev",
		"]d": "diagnosticNext",

		"[c": "gitPrev",
		"]c": "gitNext",
		"<<": "gitStage",
		">>": "gitUnstage"
	},
	"python.autoComplete.extraPaths": [
		"${env:SPARK_HOME}/python",
		"${env:SPARK_HOME}/python/pyspark"
	],
	"cSpell.userWords": [
    "CTOML",
    "Denite",
    "Nerdfont",
    "SYMDEF",
    "Semshi",
    "Tcolorscheme",
    "Unstage",
    "Valloric",
    "Yggdroot",
    "ccls",
    "clangd",
    "cohama",
    "cquery",
    "datetime",
    "eigen",
    "elzr",
    "expl",
    "funcobj",
    "jackguo",
    "kkvh",
    "kristijanhusak",
    "lexima",
    "linehl",
    "liuchengxu",
    "lldb",
    "lukas",
    "mapkey",
    "mygroup",
    "mypy",
    "neoclide",
    "nofile",
    "nord",
    "numhl",
    "numirias",
    "objc",
    "objcpp",
    "osnum",
    "pgsql",
    "pylint",
    "pyspark",
    "require'nvim",
    "sourcekit",
    "startify",
    "struct",
    "subseparator",
    "tabe",
    "tomasiser",
    "tpope",
    "ujihisa",
    "vsplit",
    "wfxr"
  ]
}
